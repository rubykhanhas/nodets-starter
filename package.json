{
  "name": "nodets",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "watch": "nodemon",
    "serve": "node ./dist/index.js"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.2"
  }
}
